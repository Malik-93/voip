    "kubectl_deployments": "kubectl get deployments",
    "kubectl_services": "kubectl get services",
    "exe": "yarn build && yarn test:darwin:unix",
    "test:darwin:unix": "pkg dist/index.js --targets node18-macos-x64 --output voip-8000"
  },
